<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "by.andersenlab.travelagency.model.Hotel" table = "HOTEL">

        <id name = "id" column = "id" type = "int">
            <generator class="native"/>
        </id>

        <property name = "nameHotel" column = "nameHotel" type = "string"/>
        <property name = "ratingStars" column = "ratingStars" type = "int"/>
    </class>
</hibernate-mapping>