<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "by.andersenlab.travelagency.model.Tour" table = "TOUR">

        <id name = "id" column = "id" type = "int">
            <generator class="native"/>
        </id>

        <property name = "nameTour" column = "nameTour" type = "string"/>
        <property name = "idCountry" column = "idCountry" type = "int"/>
        <property name = "idHotel" column = "idHotel" type = "int"/>

    </class>
</hibernate-mapping>